<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <classPathEntry
            location="D:\d\project\repo\mysql\mysql-connector-java\5.1.34\mysql-connector-java-5.1.34.jar"/>

    <context id="mysqlgenerator" targetRuntime="MyBatis3">

        <property name="javaFileEncoding" value="UTF-8"/>
        <property name="javaFormatter" value="org.mybatis.generator.api.dom.DefaultJavaFormatter"/>
        <property name="xmlFormatter" value="org.mybatis.generator.api.dom.DefaultXmlFormatter"/>

        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>
        <plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin"></plugin>

        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://10.202.125.245:3306/test?useUnicode=true&amp;characterEncoding=utf8&amp;allowMultiQueries=true"
                        userId="test" password="test"/>


        <javaModelGenerator targetPackage="com.atjl.dbtiming.domain.gen"
                            targetProject="src\main\java">
            <property name="constructorBased" value="false"/>
            <property name="enableSubPackages" value="false"/>
            <property name="immutable" value="false"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="sqlmap.gen"
                         targetProject="src\main\resources"/>
        <javaClientGenerator targetPackage="com.atjl.dbtiming.mapper.gen"
                             targetProject="src\main\java" type="XMLMAPPER"/>

        <!-- timing -->
        <table domainObjectName="GenTaskManager" schema="aossit" enableDeleteByExample="false"
               enableDeleteByPrimaryKey="false"
               tableName="ts_task_manager">
            <generatedKey column="MID" sqlStatement="Mysql" type="post" identity="true"/>
        </table>

        <table domainObjectName="GenTask" schema="aossit" enableDeleteByExample="true"
               enableDeleteByPrimaryKey="true"
               tableName="ts_task">
            <generatedKey column="TID" sqlStatement="Mysql" type="post" identity="true"/>
        </table>

        <table domainObjectName="GenTaskRuned" schema="aossit" enableDeleteByExample="true"
               enableDeleteByPrimaryKey="true"
               tableName="ts_task_runned">
            <generatedKey column="TID" sqlStatement="Mysql" type="post" identity="true"/>
        </table>

        <table domainObjectName="GenTaskHistory" schema="aossit" enableDeleteByExample="true"
               enableDeleteByPrimaryKey="true"
               tableName="ts_task_history"/>


    </context>
</generatorConfiguration>